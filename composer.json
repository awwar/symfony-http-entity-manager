{
    "name": "awwar/symfony-http-entity-manager",
    "description": "Doctrine-like http client",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "symfony/config": "~5.4",
        "symfony/dependency-injection": "~5.4",
        "symfony/http-client": "~5.4",
        "symfony/http-kernel": "~5.4",
        "symfony/finder": "~5.4",
        "adbario/php-dot-notation": "^3.1",
        "myclabs/deep-copy": "1.10.2"
    },
    "autoload": {
        "psr-4": {
            "Awwar\\SymfonyHttpEntityManager\\": "src"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8.0",
        "squizlabs/php_codesniffer": "^3.7.0"
    }
}
