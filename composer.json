{
    "name": "awwar/symfony-http-entity-manager",
    "description": "Doctrine-like http client",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "symfony/config": "~5.4|^6.0",
        "symfony/dependency-injection": "~5.4|^6.0",
        "symfony/http-client-contracts": "2.0|^3.0",
        "symfony/http-kernel": "~5.4|^6.0",
        "symfony/finder": "~5.4|^6.0",
        "adbario/php-dot-notation": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Awwar\\SymfonyHttpEntityManager\\": "src"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "*",
        "squizlabs/php_codesniffer": "*"
    }
}
